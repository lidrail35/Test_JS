{
  "parser": "@typescript-eslint/parser",
  "plugins": ["require", "@typescript-eslint"],
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
    "extends": [
      "eslint: recommended",
      "plugin: @typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": 2
    }
}